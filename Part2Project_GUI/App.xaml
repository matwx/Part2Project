<Application x:Class="Part2Project_GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:View="clr-namespace:Part2Project_GUI.View"
             xmlns:ViewModels="clr-namespace:Part2Project_GUI.ViewModel"
             Startup="App_OnStartup">
    <Application.Resources>
        <DataTemplate DataType="{x:Type ViewModels:StartScreenViewModel}">
            <View:StartScreen />
        </DataTemplate>
        <DataTemplate DataType="{x:Type ViewModels:FourImageSelectorViewModel}">
            <View:FourImageSelector />
        </DataTemplate>
    </Application.Resources>
</Application>
